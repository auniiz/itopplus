{
  "projectType": "application",
  "sourceRoot": "apps/angular/src",
  "prefix": "itopplus",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/angular",
        "index": "apps/angular/src/index.html",
        "main": "apps/angular/src/main.ts",
        "polyfills": "apps/angular/src/polyfills.ts",
        "tsConfig": "apps/angular/tsconfig.app.json",
        "assets": [
          "apps/angular/src/favicon.ico",
          "apps/angular/src/assets"
        ],
        "styles": [
          "apps/angular/src/styles.css",
          "node_modules/bootstrap/dist/css/bootstrap.min.css"
        ],
        "scripts": [],
        "serviceWorker": true,
        "ngswConfigPath": "apps/angular/ngsw-config.json"
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/angular/src/environments/environment.ts",
              "with": "apps/angular/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "angular:build:production"
        },
        "development": {
          "browserTarget": "angular:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "proxyConfig": "apps/angular/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "angular:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/angular/**/*.ts",
          "apps/angular/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/angular"
      ],
      "options": {
        "jestConfig": "apps/angular/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
